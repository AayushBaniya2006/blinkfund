{
  "rules": [
    {
      "pathPattern": "/campaigns/*",
      "apiPath": "/api/actions/donate?campaign=*"
    },
    {
      "pathPattern": "/c/*",
      "apiPath": "/api/actions/donate?campaign=*"
    },
    {
      "pathPattern": "/api/actions/donate",
      "apiPath": "/api/actions/donate"
    },
    {
      "pathPattern": "/api/actions/donate/**",
      "apiPath": "/api/actions/donate/**"
    }
  ]
}
